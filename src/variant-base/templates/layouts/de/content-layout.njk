{% block php_init %}{% endblock %}
<!DOCTYPE html>
<html lang="de">
  <head>
    {% include "../../parts/de/head.njk" %}
    {% block meta %}{% endblock %}
    {% block styles %}{% endblock %}
  </head>
{% if page_background_image | length %}
  <body style="background-image: url({{page_background_image}})">
{% else %}
  <body>
{% endif %}
    {% include "../../parts/de/header.njk" %}
    {% block slideshow %}{% endblock %}
    <main>
      {% block breadcrumb %}{% endblock %}
      <div class="content padding-bottom-normal">
        {% block content %}{% endblock %}
      </div>
      {% block more %}{% endblock %}
    </main>
    {% include "../../parts/de/footer.njk" %}
    {% block scripts %}{% endblock %}
  </body>
</html>