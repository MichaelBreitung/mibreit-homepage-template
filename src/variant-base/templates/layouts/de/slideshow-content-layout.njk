{% from "../../macros/mibreit-gallery.njk" import slideshow, prepare, randomImages, scriptSlideshow %}
{% from "../../schema/schema-gallery.njk" import imageGallery %}
{% block php_init %}  
  {{prepare()}}
  {{randomImages(8)}}
{% endblock %}
{% extends "./content-layout.njk" %}
{% block meta %}
  {{imageGallery(true)}}
{% endblock %}
{% block scripts %}
  {{scriptSlideshow(".slideshow", false, false, true)}}
{% endblock %}
{% block slideshow %}
  <div class="slideshow-container">
    {{slideshow("slideshow", true)}} 
  </div>
{% endblock %}